{
    "plugins": [
        "babel",
        "react"
    ],
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
        "node": true,
        "mocha": true,
        "es6": true,
        "jasmine": true
    },
    "globals": {
        "__DEV__": false,
        "jest": true,
        "__iOS__": false,
        "__ANDROID__": false,
        "__WEB__": false
    },
    "settings": {
        "ecmascript": 6,
        "jsx": true
    },
    "rules": {
        "no-constant-condition": 0,
        "strict": 0,
        "quotes": 0,
        "no-unused-vars": 2,
        "no-unused-state": 0,
        "no-trailing-spaces": 0,
        "no-cond-assign": 1,
        "prefer-rest-params": 0,
        "camelcase": 0,
        "no-underscore-dangle": 0,
        // 忽略indent
        "indent": 0,
        // 4空格
        "max-len": 0,
        // 单行长度
        "func-names": 0,
        // 不允许匿名函数
        "consistent-return": 0,
        // 必须有返回值
        "brace-style": 0,
        // catch, else等必须在}后面
        "no-console": 0,
        // 禁止console
        "object-shorthand": 0,
        "no-param-reassign": 0,
        // 禁止修改形参
        "new-cap": 0,
        // 非构造函数首字母不允许大写
        "global-require": 0,
        // 禁止全局require
        "no-path-concat": 0,
        //禁止__dirname直接拼接路径
        "no-restricted-syntax": 0,
        // 禁止for in
        "no-lonely-if": 0,
        "react/jsx-wrap-multilines": 0,
        //禁止else语句中只有if语句
        "react/jsx-filename-extension": 0,
        // 只能在.jsx文件使用JSX
        "react/jsx-indent": 0,
        // JSX缩进4空格
        "react/jsx-indent-props": 0,
        "react/react-in-jsx-scope": 0,
        // JSX组件属性缩进4空格
        "no-return-assign": 0,
        // 箭头函数不允许返回表达式
        "react/no-multi-comp": 0,
        // 禁止一个文件定义多个组件
        "jsx-a11y/img-has-alt": 0,
        // <img>必须有alt属性
        "require-yield": 0,
        // generator函数必须有yield
        "no-else-return": 0,
        "react/prop-types": 0,
        "no-mixed-operators": 0,
        "react/prefer-stateless-function": 0,
        "import/no-mutable-exports": 0,
        "import/no-extraneous-dependencies": 0,
        "no-use-before-define": [
            "error",
            {
                "functions": false,
                "classes": false
            }
        ],
        "no-alert": 0,
        "radix": 0,
        "eqeqeq": 0,
        "class-methods-use-this": 0,
        "comma-dangle": 0,
        "guard-for-in": 0,
        "no-amd": 0,
        "import/no-amd": 0,
        "import/no-unresolved": 0,
        "no-unused-expressions": [
            "error",
            {
                "allowShortCircuit": true
            }
        ],
        "no-plusplus": 0,
        "array-bracket-spacing": 0,
        "no-continue": 0,
        "no-extend-native": [
            "error",
            {
                "exceptions": [
                    "Date"
                ]
            }
        ],
        "no-prototype-builtins": 0,
        "spaced-comment": 0,
        "prefer-arrow-callback": 0,
        "arrow-body-style": 0,
        "eol-last": 0,
        "import/extensions": 0,
        "import/prefer-default-export": 0,
        "no-var": 0,
        "react/sort-comp": 0,
        "react/forbid-prop-types": 0,
        "react/require-default-props": 0,
        "react/no-array-index-key": 0,
        "react/jsx-no-bind": 1,
        "react/jsx-tag-spacing": 1,
        "react/jsx-curly-brace-presence": 1,
        "no-class-assign": 0,
        "no-new-func": 0,
        "react/style-prop-object": 0,
        "jsx-a11y/href-no-hash": 0,
        "no-lone-blocks": 0,
        "no-duplicate-imports": 2,
        "prefer-destructuring": 0,
        "import/first": 0,
        "no-restricted-globals": 0,
        "react/jsx-closing-tag-location": 0,
        "react/jsx-first-prop-new-line": 0,
        "react/no-unused-prop-types": 0,
        "react/jsx-closing-bracket-location": 0,
        "no-bitwise": 0
    }
}
